{
	"env": {
		"es6": true,
		"browser": true
	},
	"extends": [
		"airbnb-base",
		"plugin:prettier/recommended",
		"prettier"
		
	],
	"plugins": [
		"babel",
		"prettier"
		
	],
	"rules": {
		
		"no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
		"no-unused-vars": "off",
		"prettier/prettier": "error",
		"quotes": ["error", "double"]

	}

}
